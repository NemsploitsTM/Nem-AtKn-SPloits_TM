					end
				end
			until on == "false"
				coroutine.resume(coroutine.create(function()
					Talk("-w-")
				end))
				purring:Stop()
		on = true
		action = false
		if walking == true then
			spawn(WalkAnimation)
		elseif walking == false then
			spawn(IdleAnimation)
		end
		Character.Humanoid.WalkSpeed = 10
		Character.Humanoid.JumpPower = jumppower
end


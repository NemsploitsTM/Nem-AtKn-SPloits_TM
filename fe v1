					end
				end
			until on == "owo" or on == "false"
			on = "false"
			repeat
				if on == "false" then
					break
				end
				for i = 1, 30 do
					wait()
					if i <= 15 then
						  TailWeld.C1 = TailWeld.C1:Lerp(CFrame.new(0, 0, 0, 1, 0, 0, 0, 0.98480767, -0.173648626, 0, 0.173648626, 0.98480767), lerpintensity)

  TailWeld.C1 = TailWeld.C1:Lerp(CFrame.new(-0.0357913971, -0.133386612, 0.154045105, 0.151821822, 0.941880465, 0.299685538, 0.876633883, -0.268372595, 0.399361014, 0.456577718, 0.202082798, -0.866429031), lerpintensity)
